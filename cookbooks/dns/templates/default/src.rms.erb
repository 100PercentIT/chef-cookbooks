$TTL 900

@ 14400 IN SOA ns.rms. hostmaster.rackspace.com. (
  2010031901   ; serial
       14400   ; refresh (seconds between update requests from secondary and slave name servers)
        1800   ; retry (seconds the secondary or slave will wait before retrying when the last attempt has failed)
     1209600   ; expire (seconds a slave will wait before considering the data stale if it cannot reach the primary)
         300 ) ; minimum ttl (this is used for negative caching)

@            86400 IN NS localhost

@            86400 IN MX 10 localhost

localhost    86400 IN A 127.0.0.1
